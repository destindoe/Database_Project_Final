<Window x:Class="Dream_Home.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dream_Home"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="300">
    <Grid>
        <Label x:Name="LoginLabelHeader" Content="Dream Home" Margin="30,10,31,0" VerticalAlignment="Top" Height="68" FontSize="36" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <Label Content="Username&#xD;&#xA;" HorizontalAlignment="Left" Margin="50,110,0,0" VerticalAlignment="Top" Height="29" Width="66"/>
        <TextBox x:Name="LoginTextUsername" HorizontalAlignment="Left" Height="23" Margin="71,144,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="159"/>
        <Label Content="Password&#xD;&#xA;&#xA;" HorizontalAlignment="Left" Margin="50,180,0,0" VerticalAlignment="Top" Height="29" Width="66" RenderTransformOrigin="0.523,-0.009"/>
        <Button x:Name="LoginButtonGo" Content="Go!" Margin="115,267,103.6,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="LoginTextPassword" HorizontalAlignment="Left" Margin="71,209,0,0" VerticalAlignment="Top" Width="159"/>

    </Grid>
</Window>
